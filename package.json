{
  "name": "usepopcorn",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "setup": "pm2 start --name mk-usepopcorn-movies-rating-frontend npm -- start",
    "start": "vite preview --host --port 5185",
    "deploy": "git pull --no-rebase && npm i && npm run build && pm2 restart mk-usepopcorn-movies-rating-frontend --update-env --time && pm2 save"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  }
}
